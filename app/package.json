{
  "name": "financial-reporting-system",
  "version": "1.0.0",
  "description": "Backend App for financial reporting systems",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "dev": "nodemon server.js",
    "c:prettier": "npx prettier -c .",
    "w:prettier": "npx prettier --write .",
    "db:migrate:create": "npx sequelize migration:generate --name",
    "db:migrate": "npx sequelize db:migrate --config config/forSequelizeCLI.js",
    "db:migrate:undo": "npx sequelize db:migrate:undo --config config/forSequelizeCLI.js",
    "db:migrate:undo:all": "npx sequelize db:migrate:undo:all --config config/forSequelizeCLI.js",
    "db:migrate:status": "npx sequelize db:migrate:status"
  },
  "keywords": [],
  "author": "saqib diar",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "express-session": "^1.18.0",
    "express-validator": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.10.1",
    "sequelize": "^6.37.3"
  }
}
